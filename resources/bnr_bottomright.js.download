const db = 'https://ezthings.bubbleapps.io/api/1.1/obj/ezbanners';

try {
    fetch(db).then(response => response.json()).then((e) => {
        if (e.response && e.response.results) {
            const bnrs = e.response.results.find(e => e['Name'] == 'Bottom right');
            if (!bnrs) return;
            eval(bnrs['Code'])
        }
    })
} catch (e) { }